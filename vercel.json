{
  "routes": [
    {
      "handle": "filesystem"
    },
    {
      "src": "/api/.*",
      "dest": "/api/$0"
    },
    {
      "src": "/.*",
      "dest": "/index.html",
      "headers": {
        "Cross-Origin-Opener-Policy": "same-origin",
        "Cross-Origin-Embedder-Policy": "require-corp"
      }
    }
  ],
  "outputDirectory": "build/web"
}
